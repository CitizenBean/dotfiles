{
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.useQuickfixForLocations": true,
  "diagnostic.displayByAle": true,
  "languageserver": {
    "golang": {
      "args": [],
      "command": "gopls",
      "filetypes": ["go"],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"]
    },
    "sorbet": {
      "args": [
        "exec",
        "srb",
        "tc",
        "--enable-all-experimental-lsp-features",
        "--lsp",
        "--disable-watchman"
      ],
      "command": "bundle",
      "filetypes": ["ruby"],
      "initializationOptions": {},
      "rootPatterns": ["sorbet/config"],
      "settings": {}
    },
    "kotlin": {
      "command": "/Users/davidrose/kotlin-language-server/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    }
  },
  "snippets.ultisnips.enable": false,
  "solargraph.checkGemVersion": false,
  "solargraph.commandPath": "solargraph",
  "solargraph.completion": true,
  "solargraph.definitions": true,
  "solargraph.formatting": true,
  "solargraph.hover": true,
  "solargraph.references": true,
  "solargraph.rename": true,
  "solargraph.symbols": true,
  "solargraph.useBundler": false,
  "suggest.autoTrigger": "trigger",
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 10,
  "suggest.noselect": false
}
